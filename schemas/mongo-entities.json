{
  "title": "Entities",
  "description": "Entities collection on MongoDB",
  "type": "object",
  "required": ["id", "parameters"],
  "additionalProperties": false,
  "properties": {
    "_id": {
      "bsonType": "objectId"
    },
    "id": {
      "type": "string",
      "pattern": "^\\w+$"
    },
    "createdAt": {
      "bsonType": "date",
      "format": "date-time",
      "instanceOf": "Date"
    },
    "updatedAt": {
      "bsonType": "date",
      "format": "date-time",
      "instanceOf": "Date"
    },
    "parameters": {
      "type": "array",
      "minItems": 20,
      "maxItems": 20,
      "additionalItems": false,
      "items": {
        "bsonType": "decimal",
        "minimum": -1,
        "maximum": 1
      }
    }
  }
}
